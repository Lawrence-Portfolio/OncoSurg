#search-result
    ul
        list-style-type: none;
        padding-left 0;
        li
            margin-bottom 20px;
            &:last-child
                margin-bottom 0;
            a
                color: #000;
                text-decoration: underline;
                font-size: 22px;
                transition: all .2s ease;
                &:hover
                    text-decoration none;
.search-fail
    display: block;
    font-size: 22px;
    margin-bottom 20px;
